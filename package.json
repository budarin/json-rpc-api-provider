{
    "name": "@budarin/json-rpc-api-provider",
    "version": "1.2.4",
    "repository": {
        "url": "git+https://github.com/budarin/json-rpc-api-provider.git"
    },
    "author": "Vadim Budarin <budarin-vv@yandex.ru>",
    "license": "MIT",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
